{
    "name": "liking/select2-filter",
    "description": "Nova Select2 Filter",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "select2 filter"
    ],
    "authors": [
        {
            "name": "liking",
            "email": "m.linhking@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Liking\\Select2Filter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liking\\Select2Filter\\FilterServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "vue-select",
                "version": "^3.0.2",
                "type": "package",
                "source": {
                    "type": "git",
                    "reference": "3.0.2",
                    "url": "https://github.com/sagalbot/vue-select"
                }
            }
        }
    ],
    "require": {},
    "scripts": {
        "post-install-cmd": [
            "bower install"
        ],
        "post-update-cmd": [
            "bower install"
        ]
    }
}
